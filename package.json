{
  "name": "automated-beat-generation",
  "version": "1.0.0",
  "description": "Automated beat generation system using AI APIs",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "dev:api": "ts-node-dev --respawn --transpile-only src/api/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:api": "node dist/api/index.js",
    "test": "jest",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:property": "jest --testPathPattern=tests/property",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:coverage": "jest --coverage",
    "test:workflow": "ts-node scripts/test-workflow.ts",
    "validate": "ts-node scripts/final-validation.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "import:keys": "ts-node scripts/import-api-keys.ts",
    "check:keys": "ts-node scripts/check-api-keys.ts",
    "setup:admin": "ts-node scripts/setup-admin.ts",
    "backup": "ts-node scripts/backup-database.ts",
    "restore": "ts-node scripts/restore-database.ts",
    "docker:build": "docker build -t ai-beat-generator .",
    "docker:up": "docker-compose --env-file .env.production up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": [
    "beat",
    "music",
    "ai",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@prisma/client": "^5.22.0",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/multer": "^2.0.0",
    "axios": "^1.6.2",
    "bcryptjs": "^3.0.3",
    "canvas": "^3.2.0",
    "chokidar": "^3.5.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.22.1",
    "express-rate-limit": "^7.5.1",
    "fluent-ffmpeg": "^2.1.3",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "multer": "^2.0.2",
    "music-tempo": "^1.0.3",
    "node-cron": "^3.0.3",
    "openai": "^6.10.0",
    "web-audio-beat-detector": "^8.2.31",
    "winston": "^3.11.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/image-size": "^0.7.0",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.26",
    "@types/node-cron": "^3.0.11",
    "@types/xml2js": "^0.4.14",
    "fast-check": "^3.15.0",
    "image-size": "^2.0.2",
    "jest": "^29.7.0",
    "prisma": "^5.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
